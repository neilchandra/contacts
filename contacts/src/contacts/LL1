AdressBook ::= adressbook TopGroupList

TopGroupList ::= closeadressbook
				| opengroup GroupHelper TopGroupList

GroupHelper ::= opencontact Contact GroupHelper
				| SubGroupList

SubGroupList ::= closegroup
				 | opengroup GroupHelper SubGroupList
				 | GroupHelper

Contact ::= Name Number OwnID openFriends FriendsHelper

Name ::= "STRING" closename
			  | closename
			  
Number ::= "STRING" closenumber
				| closenumber
				
OwnId ::= NUMBER closeownid
				| closeownid
				   
Friends ::= | closefriends closeContact
			  | openid NUMBER closeid FriendsID

